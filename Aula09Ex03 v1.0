#include <stdio.h>
#include <stdlib.h>


int main (){
int soma,i,j;
int v[2][2];
soma = 0;

for(i=0 ; i < 2 ; i++){
    for(j=0 ; j < 2 ; j++){
        v[i][j] = (i+2)*(j+2);
    }
}
for(i=0 ; i < 2 ; i++){
    for(j=0 ; j < 2 ; j++){
        soma = soma + v[i][j];
    }
}

printf("A soma e: %d", soma);
return 0;
}
