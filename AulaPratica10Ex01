#include <stdio.h>
#include <stdlib.h>

void troca(int a, int b){
    int c;
    int *pa,*pb;
    pa = &a;
    pb = &b;
        c = a;
        *pa = b;
        *pb = c;
}

int main(){
int num1,num2;

num1 = 2;
num2 = 5;

troca(num1,num2);

printf("numero 1 e: %d\nnumero 2 e: %d", num1, num2);
return 0;
}
